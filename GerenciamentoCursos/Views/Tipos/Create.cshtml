@model GerenciamentoCursos.Models.Tipo
@{
    ViewData["Title"] = "Create";
}
<h2>@ViewData["Title"]</h2>
<h4>Tipos</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <label asp-for="TipoCurso" class="control-label"></label>
                <input asp-for="TipoCurso" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default">
            </div>
        </form>
    </div>    
</div>
<div class="row col-md-12"></div>

